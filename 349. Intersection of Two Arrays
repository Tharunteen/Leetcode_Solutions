class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> a=new HashSet<>();
        for(int i:nums1){
            a.add(i);
        }
        Set<Integer> b=new HashSet<>();
        for(int i:nums2){
            b.add(i);
        }
        List<Integer> c=new ArrayList<>();
        if(a.size()>b.size()){
                for(int i:b){
                    if(a.contains(i)) {
                        c.add(i);
                    }
                }
        }else{
                for(int i:a){
                    if(b.contains(i)) {
                        c.add(i);
                    }
                }
        }
        return c.stream().mapToInt(Integer::intValue).toArray();
    }
}
